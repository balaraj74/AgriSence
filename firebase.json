
{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "public",
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "**/*.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=60"
          }
        ]
      }
    ],
    "authDomain": "agrisence-1dc30.firebaseapp.com"
  }
}
